{
  "name": "civic-dashboard-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && tsc --noEmit",
    "test:unit": "vitest run",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "preview:remote": "opennextjs-cloudflare build && opennextjs-cloudflare preview --remote",
    "preview-remote:worker": "opennextjs-cloudflare build && wrangler versions upload",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "db:update-types": "kysely-codegen --out-file ./src/database/generatedDbTypes.d.ts --url $DATABASE_URL && eslint ./src/database/generatedDbTypes.d.ts --fix",
    "db:create-migration": "kysely migrate:make",
    "db:list-migrations": "kysely migrate:list",
    "db:run-migrations": "kysely migrate:latest && kysely seed:run",
    "db:run-seed": "kysely seed:run",
    "db:start": "docker compose up --detach database",
    "db:stop": "docker compose stop database",
    "docker:start": "docker compose up --detach",
    "docker:stop": "docker compose stop",
    "tsx": "tsx --tsconfig=tsconfig.tsx.json",
    "tsxe": "tsx --tsconfig=tsconfig.tsx.json --env-file=.env",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-email/components": "^0.0.29",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "csv-parse": "^5.5.6",
    "hash-sum": "2.0.0",
    "isomorphic-dompurify": "^2.22.0",
    "kysely": "^0.27.5",
    "kysely-postgres-js": "^2.0.0",
    "lucide-react": "^0.460.0",
    "mark.js": "^8.11.1",
    "next": "^14.2.18",
    "next-sitemap": "^4.2.3",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "resend": "^4.0.1",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@cypress/webpack-dev-server": "^5.1.2",
    "@eslint/js": "^9.16.0",
    "@opennextjs/cloudflare": "^1.8.2",
    "@tailwindcss/typography": "^0.5.15",
    "@types/hash-sum": "1.0.2",
    "@types/mark.js": "^8.11.12",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/umami": "^2.10.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-sql": "^3.2.1",
    "kysely-codegen": "^0.17.0",
    "kysely-ctl": "^0.10.1",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.2",
    "postcss": "^8",
    "prettier": "3.4.2",
    "react-email": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.3",
    "typescript": "^5",
    "typescript-eslint": "^8.18.0",
    "vitest": "^3.2.4",
    "webpack": "^5.101.3",
    "wrangler": "^4.37.1"
  }
}
