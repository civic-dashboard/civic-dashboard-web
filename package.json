{
  "name": "civic-dashboard-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:worker": "opennextjs-cloudflare",
    "dev:worker": "wrangler dev --port 8771",
    "preview-local:worker": "npm run build:worker && npm run dev:worker",
    "preview-remote:worker": "npm run build:worker && wrangler versions upload",
    "deploy:worker": "npm run build:worker && wrangler deploy",
    "db:update-types": "kysely-codegen --out-file ./src/database/generatedDbTypes.d.ts --url $DATABASE_URL && eslint ./src/database/generatedDbTypes.d.ts --fix",
    "db:create-migration": "kysely migrate:make",
    "db:list-migrations": "kysely migrate:list",
    "db:run-migrations": "kysely migrate:latest",
    "db:start": "docker compose up --detach",
    "db:stop": "docker compose stop"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-email/components": "^0.0.29",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "csv-parse": "^5.5.6",
    "cmdk": "^1.0.0",
    "flexsearch": "^0.7.43",
    "hash-sum": "2.0.0",
    "kysely": "^0.27.5",
    "kysely-postgres-js": "^2.0.0",
    "lucide-react": "^0.460.0",
    "mark.js": "^8.11.1",
    "next": "^14.2.18",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^4.0.1",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@opennextjs/cloudflare": "^0.4.4",
    "@types/hash-sum": "1.0.2",
    "@tailwindcss/typography": "^0.5.15",
    "@types/mark.js": "^8.11.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-sql": "^3.2.1",
    "kysely-codegen": "^0.17.0",
    "kysely-ctl": "^0.10.1",
    "pg": "^8.13.2",
    "postcss": "^8",
    "prettier": "3.4.2",
    "react-email": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "typescript-eslint": "^8.18.0",
    "wrangler": "^3.91.0"
  }
}
